{
  "name": "presentation-helper",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "validate-powerpoint": "office-toolbox validate -m ./src/assets/powerpoint-manifest.xml",
    "validate-excel": "office-toolbox validate -m ./src/assets/excel-manifest.xml",
    "sideload-powerpoint": "office-toolbox sideload -m ./src/assets/powerpoint-manifest.xml -a PowerPoint",
    "sideload-excel": "office-toolbox sideload -m ./src/assets/excel-manifest.xml -a Excel",
    "bust-powerpoint-cache": "rm ~/Library/Containers/com.microsoft.Powerpoint/Data/documents/wef/powerpoint-manifest.xml",
    "bust-excel-cache": "rm ~/Library/Containers/com.microsoft.Excel/Data/documents/wef/excel-manifest.xml",
    "validate-powerpoint-prod": "office-toolbox validate -m ./src/assets/powerpoint-prod-manifest.xml",
    "validate-excel-prod": "office-toolbox validate -m ./src/assets/excel-prod-manifest.xml",
    "sideload-powerpoint-prod": "office-toolbox sideload -m ./src/assets/powerpoint-prod-manifest.xml -a PowerPoint",
    "sideload-excel-prod": "office-toolbox sideload -m ./src/assets/excel-prod-manifest.xml -a Excel",
    "bust-powerpoint-cache-prod": "rm ~/Library/Containers/com.microsoft.Powerpoint/Data/documents/wef/powerpoint-prod-manifest.xml",
    "bust-excel-cache-prod": "rm ~/Library/Containers/com.microsoft.Excel/Data/documents/wef/excel-prod-manifest.xml"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "@microsoft/office-js": "^1.1.10",
    "bootstrap": "^4.3.1",
    "core-js": "^2.5.4",
    "jquery": "^3.3.1",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.12.0",
    "@angular/cli": "~7.2.0",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "@types/office-js": "0.0.159",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "office-toolbox": "^0.1.1",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2"
  }
}
